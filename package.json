{
  "name": "electron-react-express-boilerplate",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "main": "src/electron.js",
  "proxy": "http://localhost:3001",
  "module": "",
  "build": {
    "appId": "be.patryksitko.electron-react-express-boilerplate",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      {
        "from": "src",
        "to": "build",
        "filter": "**/*"
      }
    ],
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/register": "^7.13.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "^4.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.3.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "node-sass": "^4.0.0",
    "react": "^17.0.1",
    "react-app-rewired": "^2.1.8",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "redux": "^4.0.5",
    "redux-first-routing": "^0.3.0",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-start": "node src/start-react",
    "react-start": "cross-env BROWSER=none react-scripts start",
    "express-start": "node src/express/bin/www.mjs",
    "pack": "electron-builder build --dir",
    "dist": "npm run build && electron-builder build",
    "postinstall": "install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1"
  }
}
